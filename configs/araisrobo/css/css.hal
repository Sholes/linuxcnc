net j1-2-risc-probe-pin                           => wou.stepgen.2.risc-probe-pin
net j3-risc-probe-pin    joint.3.risc-probe-pin => wou.stepgen.3.risc-probe-pin
net j4-risc-probe-pin    joint.4.risc-probe-pin => wou.stepgen.4.risc-probe-pin

net j1-2-risc-probe-sel      newmux2s.1.sel          
net j1-risc-probe-type       joint.1.risc-probe-type => newmux2s.1.in1
net j2-risc-probe-type       joint.2.risc-probe-type => newmux2s.1.in2
net j1-2-risc-probe-type                             <= newmux2s.1.out

net j0-risc-probe-type   joint.0.risc-probe-type => wou.stepgen.0.risc-probe-type
