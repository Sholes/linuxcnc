net j1-2-risc-probe-sel     newmux2s.0.sel          
net j1-risc-probe-pin       joint.1.risc-probe-pin => newmux2s.0.in1
net j2-risc-probe-pin       joint.2.risc-probe-pin => newmux2s.0.in2
net j1-2-risc-probe-pin                            <= newmux2s.0.out

net j0-risc-probe-pin    joint.0.risc-probe-pin => wou.stepgen.0.risc-probe-pin
